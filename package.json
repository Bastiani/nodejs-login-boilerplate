{
  "name": "nodejs-login-boilerplate",
  "version": "1.0.0",
  "description": "boilerplate for start a project with nodejs/koa",
  "main": "src/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "dev": "nodemon src/index.ts",
    "build": "sucrase ./src -d ./dist --transforms typescript,imports",
    "prod": "tsc --skipLibCheck"
  },
  "keywords": [
    "koa",
    "babel",
    "boilerplate",
    "scaffold",
    "es6",
    "es2015",
    "es2016",
    "es2017",
    "jest",
    "eslint"
  ],
  "author": "Rafael Bastiani <rafacdb@gmail.com> (https://rafaelbastiani.com/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bastiani/nodejs-login-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/Bastiani/nodejs-login-boilerplate/issues"
  },
  "homepage": "https://github.com/Bastiani/nodejs-login-boilerplate#readme",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/graphql": "^14.2.2",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/koa": "^2.0.49",
    "@types/koa-convert": "^1.2.2",
    "@types/koa-cors": "^0.0.0",
    "@types/koa-graphql": "^0.8.0",
    "@types/koa-router": "^7.0.42",
    "@types/mongoose": "^5.5.8",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.2",
    "sucrase": "^3.10.1",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@entria/graphql-mongoose-loader": "^2.0.0",
    "bcryptjs": "^2.4.3",
