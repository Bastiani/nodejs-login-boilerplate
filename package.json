    "jsonwebtoken": "^8.3.0",
    "koa": "^2.4.1",
    "koa-convert": "^1.2.0",
    "koa-cors": "^0.0.16",
    "koa-graphql": "^0.7.5",
    "koa-router": "^7.3.0",
    "mongoose": "^5.1.5",
    "nodemon": "^1.17.5"
  }
